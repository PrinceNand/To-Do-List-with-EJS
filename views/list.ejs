

<%- include("header")-%>   

    <div class="box" id="heading">
        <!-- Eventday value will come from app.js day as key value pair -->
    <h1><%=titleList%></h1>
    </div>
 
    

    <div class="box">
        <% for(let i=0 ; i<newListitem.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newListitem[i] %></p>
            </div>
            
       <% } %>

       <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="Add new Item" required autocomplete="off">
        <button type="submit" name="list" value="<%= titleList %>">+</button>
    </form>
        </div>

    
        <%- include("footer")-%>